    "extract-text-webpack-plugin": "~3.0.1",
    "file-loader": "^1.1.5",
    "file-saver": "^1.3.3",
    "find-root": "^1.1.0",
    "font-awesome": "^4.7.0",
    "fs-extra": "^4.0.2",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "ie-shim": "^0.1.0",
    "imports-loader": "^0.7.1",
    "inline": "0.0.2",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine-core": "^2.8.0",
    "json-loader": "^0.5.7",
    "jspdf": "^1.3.5",
    "jspdf-autotable": "^2.3.2",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
