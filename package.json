    "ngc-webpack": "~3.2.2",
    "node-sass": "^4.5.3",
    "npm": "~5.5.1",
    "npm-run-all": "^4.1.1",
    "optimize-js-plugin": "0.0.4",
    "parse5": "^3.0.2",
