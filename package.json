{
  "name": "ng2-qgrid",
  "version": "11.2.0",
  "scripts": {
    "build:app": "npm run clean && node build.theme.js && ng build --prod --output-path dist/app --base-href /ng2/",
    "build:lib": "npm run clean && node build.lib.js",
    "clean": "rimraf dist",
    "lint": "ng lint",
    "ng": "ng",
    "preinstall": "npm i glob && node preinstall.js",
    "postinstall": "npm run clean",
    "pretest:screenshot": "webdriver-manager update --gecko=false && if not exist e2e\\diff mkdir e2e\\diff",
    "release:app": "npm run build:app && npx angular-cli-ghpages --dir=dist/app",
    "release:example": "node ./build.example",
    "release:lib": "npm run build:lib && cd dist/ng2-qgrid && npm publish",
    "start:prod": "node build.serve --prod",
    "start": "node build.serve",
    "test:core": "karma start ./projects/qgrid-core/karma.conf.js --single-run",
    "test:core-watch": "karma start ./projects/qgrid-core/karma.conf.js ",
    "test:screenshot-update-goldens": "protractor ./e2e/protractor-update-goldens.conf.js",
    "test:screenshot": "protractor ./e2e/protractor.conf.js"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~11.2.0",
    "@angular/common": "~11.2.0",
    "@angular/compiler": "~11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/forms": "~11.2.0",
    "@angular/platform-browser": "~11.2.0",
    "@angular/platform-browser-dynamic": "~11.2.0",
    "core-js": "^3.8.3",
    "rxjs": "~6.6.3",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.3",
    "@qgrid/core": "file:./projects/qgrid-core",
    "@qgrid/plugins": "file:./projects/qgrid-plugins",
    "@qgrid/ngx": "file:./dist/ngx-qgrid",
    "@qgrid/ngx-plugins": "file:./dist/ngx-qgrid-plugins"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.0",
    "@angular-devkit/build-ng-packagr": "0.1002.0",
    "@angular/cdk": "^11.2.0",
    "@angular/cli": "^11.2.0",
    "@angular/compiler-cli": "~11.2.0",
    "@angular/language-service": "~11.2.0",
    "@angular/material": "^11.2.0",
    "@angular/router": "~11.2.0",
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/cucumber": "^6.0.1",
    "@types/lodash-es": "^4.17.4",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.27",
    "angular-cli-ghpages": "^0.6.2",
    "babel-loader": "^8.2.2",
    "blue-harvest": "^0.3.1",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^1.0.0",
    "classlist.js": "^1.1.20150312",
    "codelyzer": "^6.0.1",
    "command-line-args": "^5.1.1",
    "css.escape": "^1.5.1",
    "cucumber": "^6.0.5",
    "fastdom": "^1.0.10",
    "file-saver": "^2.0.5",
    "glob": "^7.1.6",
    "karma": "~5.2.3",
    "karma-chai": "^0.1.0",
    "karma-chai-spies": "^0.1.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^5.0.0",
    "livr": "2.3.1",
    "lodash-es": "^4.17.20",
    "mocha": "^8.3.0",
    "ng-packagr": "^11.2.1",
    "npm-sass": "^2.3.0",
    "protractor": "~7.0.0",
    "protractor-cucumber-framework": "^6.2.1",
    "sane": "^4.1.0",
    "shelljs": "^0.8.4",
    "ts-node": "~9.1.1",
    "tsickle": "^0.39.1",
    "tslint": "~6.1.3",
    "typescript": "~4.1.5",
    "webpack": "^4.44.2",
    "xlsx": "^0.16.9"
  }
}
