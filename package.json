    "webpack-angular-externals": "^1.0.2",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "~2.9.2",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-merge": "~4.1.0",
    "webpack-rxjs-externals": "^1.0.0",
